{
  "mcpServers": {
    "gitkraken": {
      "command": "bash",
      "args": ["./scripts/mcp-gitkraken"]
    },
    "filesystem": {
      "command": "bash",
      "args": ["./scripts/mcp-filesystem"]
    },
    "context7": {
      "command": "bash",
      "args": ["./scripts/mcp-context7"]
    },
    "stitch": {
